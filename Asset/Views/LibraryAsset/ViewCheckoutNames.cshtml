@model Asset.ViewModel.AssetVM.ListCheckoutNames
<div class="col-md-10 offset-md-1">
    <h4 class="text-center">My Penalties</h4>
    <table class="table table-hover table-bordered">
        <thead class="bg-dark">
            <tr class="text-center">
                <td>Patron Name</td>
                <td>Book Name</td>
                <td>Date of Checkout</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListNames)
            {
            <tr class="text-center">

                <td>@item.PatronName</td>
                <td>@item.AssetName</td>
                <td>@item.CheckoutedDate</td>

            </tr>
            }
        </tbody>
    </table>
</div>
