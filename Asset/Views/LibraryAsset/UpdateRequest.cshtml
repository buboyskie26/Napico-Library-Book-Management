@model Asset.ViewModel.CheckoutRequestVM.RequestIndex;


<div class="container">
    <div class="row">

        <div class="form-group">
            <a asp-action="ApproveRequest" asp-route-id="@Model.Id" class="btn btn-success">
                <i class="fa fa-check"></i> Approved

            </a>
            <a asp-action="DeclinedRequest" asp-route-id="@Model.Id" class="btn btn-danger">
                <i class="fa fa-times"></i> Decline

            </a>
        </div>
    </div>
</div>

